{
  "name": "create-plop",
  "version": "0.0.1",
  "description": "Generator for creating plopfile.js for your project",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "build": "tsc -p tsconfig.json",
    "clean": "node ./clean.js",
    "docs": "typedoc --entryPoints src/main.ts",
    "ts-node": "ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+github.com:NEWBRAN-LTD/create-plop.git"
  },
  "keywords": [
    "node",
    "plop",
    "plopfile.js",
    "team",
    "collaboration"
  ],
  "bin": {
    "create-plop": "./index.js"
  },
  "author": "Joel Chu <joelchu@foxmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "plop": "^3.0.5",
    "@ava/typescript": "^1.1.1",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "ava": "^3.15.0",
    "esbuild": "^0.12.0",
    "esbuild-register": "^2.5.0",
    "eslint": "^7.27.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "aws-v-cli": "^0.2.3",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "tslib": "^1.14.1",
    "yargs": "^16.2.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ]
  }
}
